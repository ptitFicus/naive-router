    "spa"
  ],
  "author": "Benjamin Cavy",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ptitFicus/naive-router/issues"
  },
  "homepage": "https://github.com/ptitFicus/naive-router#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
